<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.teamwebboard.mapper.RequesterMapper">
    <select id="selectRequesterList"  resultType="org.zerock.teamwebboard.domain.Requester">
        select *
        from tbl_requester
        order by rno asc
            limit #{skip},#{size}
    </select>
    <select id="getRequesterTotal" resultType="int">
        select count(rno) from tbl_requester
    </select>



</mapper>
